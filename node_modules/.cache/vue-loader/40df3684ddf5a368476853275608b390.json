    // 添加id，为图片设置顺序，效率高\r\n                    // {id:1,img_url:\"http://127.0.0.1:3000/img/banner1.png\"},\r\n                    // 将图片保存在服务器中\r\n                    // {id:2,img_url:\"http://127.0.0.1:3000/img/banner2.png\"},\r\n                    // {id:3,img_url:\"http://127.0.0.1:3000/img/banner3.png\"},\r\n                    // {id:4,img_url:\"http://127.0.0.1:3000/img/banner4.png\"},\r\n                ]\r\n            }\r\n        },\r\n        methods:{\r\n            getImages(){\r\n                //完成一个功能，获取服务器端轮播图片\r\n                 //1.发送ajax请求\r\n                 var url=\"http://127.0.0.1:3000/getImages\";\r\n                 this.axios.get(url).then(result=>{\r\n                     this.list=result.data;\r\n                 })\r\n                 //2.获取返回数据保存list\r\n            }\r\n        },\r\n        created(){\r\n            //当组件对象创建成功后即可发送ajax请求\r\n            this.getImages();\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n/*轮播图设置父元素高度*/\r\n.app-home .mint-swipe{\r\n    height:200px;\r\n}\r\n.app-home .mint-swipe img{\r\n    width:100%;\r\n}\r\n/*九宫格 图片大小 dpr 2*/\r\n.app-home .mui-grid-9 img{\r\n    width:60px;\r\n    height:60px;\r\n}\r\n/*九宫格 背景白色 */\r\n.app-home .mui-grid-9{\r\n    background-color:#fff !important;\r\n}\r\n.app-home .mui-grid-9 li{\r\n    border:0 !important;\r\n}\r\n</style>"]}]}